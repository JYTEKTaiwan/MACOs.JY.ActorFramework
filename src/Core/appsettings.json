{
  "MACOs.Services": {
    "Devices": [
      {
        "Core": {
          "Path": ".\\HAS_DEMO_net472.dll",
          "Type": "TestServiceContext",
          "Parameters": []
        },
        "DataBus": {
          "Path": ".\\MACOs.Services.dll",
          "Type": "NetMQDataBusContext",
          "Parameters": {
            "BeaconPort": 9999,
            "BeaconIP": "",
            "AliasName": "DEMO",
            "LocalIP": "tcp://127.0.0.1",
            "Port": 1234
          }
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "./log/${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Off",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logfile"
      }
    ]
  }


}
 
